opengt
 set com 115200n81
 set senddelay 0.02
 waitquiet 1 0.2
 flash 0.1

:start
 send "AT+COPS?^m"
 waitfor 15 "CHN-CT","CMCC","CHN-UNICOM","ERR"
 if % = 0 goto chnct
 if % = 1 goto cmcc
 if % = 2 goto chnunicom
 if % = 3 goto error

 print "Timeout dialing\n"

 exit 1

:chnct
 print "send China Telecom dial command\n"
 send "AT$QCRMCALL=1,1,,,,,,\"ctnet@mycdma.cn\",\"vnet.mobi\"^m"
 exit 0

:cmcc
 print "send China Mobile dial command\n"
 send "AT$QCRMCALL=1,1^m"
 exit 0

:chnunicom
 print "send China Unicom dial command\n"
 send "AT$QCRMCALL=1,1^m"
 exit 0

:timeout
 print "waitfor AT+COPS? timeout"
 exit 1

:error
 print "query AT+COPS? error"
 exit 1

