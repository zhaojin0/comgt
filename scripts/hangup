opengt
  set com 115200n81
  set senddelay 0.02
  waitquiet 1 0.2
  flash 0.1

  print "Hangup SIMCOM NDIS QCRMCALL\n"
  send "AT$QCRMCALL=0,1^m"

  waitfor 15 "OK","ERR","ERROR"
  if % = 0 goto continue
  if % = 1 goto senderror
  if % = 2 goto senderror

  print "Timeout dialing AT$QCRMCALL'\n"
  system "rm /tmp/simcom.lock"
  exit 1

:senderror
 system "rm /tmp/simcom.lock"
 print "Hnagup Error\n"
 exit 1

:continue
 system "rm /tmp/simcom.lock"
 exit 0

